#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	cout<<"CEIL: "<<ceil(2.3)<<endl;
	cout<<"FLOOR: "<<floor(2.3)<<endl;
	cout<<"EXP: "<<exp(2.3)<<endl;
	cout<<"LOG: "<<log(2.3)<<endl;
	cout<<"LOG10 : "<<log10(2.3)<<endl;
	cout<<"POW : "<<pow(2,3)<<endl;
	cout<<"SIN: "<<sin(2.3)<<endl;
	cout<<"CAN: "<<cos(2.3)<<endl;
	cout<<"TAN: "<<tan(2.3)<<endl;
	cout<<"SQRT: "<<sqrt(2.3)<<endl;


}


